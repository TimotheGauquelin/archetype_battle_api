{
  "name": "archetype_battle_api",
  "version": "1.0.0",
  "description": "API Node.js avec Express, TypeScript, Sequelize et PostgreSQL",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "husky",
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src config --ext .ts,.js --max-warnings=0",
    "lint:fix": "eslint src config --ext .ts,.js --fix",
    "format": "prettier --write \"src/**/*.{ts,js}\" \"config/**/*.{ts,js}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js}\" \"config/**/*.{ts,js}\""
  },
  "keywords": [
    "express",
    "typescript",
    "sequelize",
    "postgresql"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.13.1",
    "sequelize": "^6.37.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.10.2",
    "@types/pg": "^8.11.10",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2"
  }
}
